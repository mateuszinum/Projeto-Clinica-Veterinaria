        <property name="styleSheet">
         <string notr="true">/* Fundo geral do calendário */
QCalendarWidget {
    background-color: white;
    color: #000000;
    border-radius: 5px;
}

/* Header do calendário */
#qt_calendar_navigationbar {
    background-color: #00bcd4;
    border: none;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

/* Botões de navegação */
#qt_calendar_prevmonth,
#qt_calendar_nextmonth {
    border: none;
    qproperty-icon: none;
    min-width: 13px;
    max-width: 13px;
    min-height: 13px;
    max-height: 13px;
    background-color: transparent;
    padding: 5px;
    border-radius: 4px;
}

#qt_calendar_prevmonth:hover,
#qt_calendar_nextmonth:hover {
    background-color: #4dd0e1;
}

#qt_calendar_prevmonth:pressed,
#qt_calendar_nextmonth:pressed {
    background-color: #b2ebf2;
}

#qt_calendar_prevmonth {
    image: url(:/icon/arrow-119-48.ico);
    margin-left: 5px;
}

#qt_calendar_nextmonth {
    image: url(:/icon/arrow-19-48.ico);
    margin-right: 5px;
}

/* Botões de mês e ano */
#qt_calendar_yearbutton,
#qt_calendar_monthbutton {
    color: #000;
    font-size: 13px;
    border-radius: 4px;
    background-color: #00bcd4;
    padding: 4px 8px;
}

#qt_calendar_yearbutton:hover,
#qt_calendar_monthbutton:hover {
    background-color: #e0f7fa;
}

/* Campo de edição de ano */
#qt_calendar_yearedit {
    color: #000;
    background: transparent;
    font-size: 13px;
}

/* Botões de incremento do ano */
#qt_calendar_yearedit::up-button {
    image: url(:/icon/arrow-151-48.ico);
}

#qt_calendar_yearedit::down-button {
    image: url(:/icon/arrow-213-48.ico);
}

#qt_calendar_yearedit::up-button,
#qt_calendar_yearedit::down-button {
    width: 10px;
    padding: 0px 4px;
    border-radius: 3px;
}

#qt_calendar_yearedit::up-button:hover,
#qt_calendar_yearedit::down-button:hover {
    background-color: #e0f7fa;
}

/* Grid de datas */
#qt_calendar_calendarview {
    outline: 0px;
    background-color: white;
    border: 2px solid #00bcd4;
    border-top: 0px;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}

/* Datas hover e selecionadas */
#qt_calendar_calendarview::item:hover {
    background-color: #b2ebf2;
    border-radius: 4px;
    color: #000000;
}

#qt_calendar_calendarview::item:selected {
    background-color: #4db6ac;
    border-radius: 4px;
    color: #000000;
}

/* Texto padrão das datas */
QCalendarWidget QAbstractItemView::item:enabled {
    color: #000000;
}

/* Dias de outros meses (desabilitados) em cinza */
QCalendarWidget QAbstractItemView:disabled {
    color: #a0a0a0;
    background-color: #e0e0e0;
}</string>
